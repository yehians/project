@model IEnumerable<WebApplication1.Models.Table>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Table_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Char_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Avilibality)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Table_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Char_number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Avilibality)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
